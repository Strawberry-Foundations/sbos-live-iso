#!/bin/sh
# Description: Set Vanilla OS name and info

# Prepare
distro_pretty_name="@DISTRO_NAME Snapshot @VERSION"

# Set OS info
echo 'PRETTY_NAME="'$distro_pretty_name'"
NAME="@DISTRO_NAME"
VERSION_ID="@VERSION"
VERSION="@VERSION @CHANNEL"
VERSION_CODENAME="@CODENAME"
ID=strawberryos
HOME_URL="https://strawberryfoundations.xyz"
SUPPORT_URL="https://github.com/Strawberry-Foundations"
BUG_REPORT_URL="https://github.com/Strawberry-Foundations"' > /etc/os-release
