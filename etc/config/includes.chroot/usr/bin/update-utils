#!/bin/bash

wget https://raw.githubusercontent.com/Strawberry-Foundations/sbos-scripts/main/os-bootstrap -O /usr/local/bin/os-bootstrap
chmod a+x /usr/local/bin/os-bootstrap

wget https://raw.githubusercontent.com/Strawberry-Foundations/sbos-scripts/main/sbos-chroot -O /usr/local/bin/sbos-chroot
chmod a+x /usr/local/bin/sbos-chroot

vwget https://raw.githubusercontent.com/Strawberry-Foundations/sbos-scripts/main/neofetch -O /usr/local/bin/neofetch
chmod a+x /usr/local/bin/neofetch